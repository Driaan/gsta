                        <Grid>
                            <local:timesheetUserControl d:IsLocked="True" />
                            <Grid x:Name="notifyGrid"
                                  Height="0"
                                  VerticalAlignment="Bottom"
                                  Background="#FFFF814C">
                                <Label x:Name="label10"
