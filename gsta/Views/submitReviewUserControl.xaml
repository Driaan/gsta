<UserControl x:Class="submitReviewUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:gsta"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="350"
             Height="465"
             d:DesignHeight="300"
             d:DesignWidth="300"
             DataContext="{DynamicResource ApplicationViewModel}"
             FontFamily="{DynamicResource robotoRegularFont}"
             mc:Ignorable="d">
    <Grid Background="#FFFF814C">
        <Label x:Name="label"
               Margin="10,30,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="Current Period"
               Foreground="#DDFFFFFF" />
    </Grid>
</UserControl>