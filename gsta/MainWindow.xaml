                                <local:emailUserControl Height="Auto" />
                                <Button x:Name="improveCloseButton"
                                        Width="30"
                                        Height="30"
                                        Margin="0,0,5,175"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Bottom"
                                        Background="White"
                                        BorderBrush="{x:Null}"
                                        BorderThickness="0"
                                        Foreground="{Binding _accentSolidColorBrush}"
                                        Padding="0"
                                        Style="{DynamicResource textButtonStyle}">
                                    <Grid x:Name="improveCloseGrid">
                                        <Border x:Name="improveCloseBorder"
                                                Width="30"
                                                Height="30">
                                            <Rectangle Fill="White" StrokeThickness="0" />
                                        </Border>
                                        <materialDesign:PackIcon Width="Auto"
                                                                 Height="Auto"
                                                                 HorizontalAlignment="Stretch"
                                                                 VerticalAlignment="Stretch"
                                                                 Kind="ArrowUp" />
                                    </Grid>
                                </Button>
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>
            <Grid x:Name="handleGrid"
                  Width="350"
                  Height="25"
                  VerticalAlignment="Top"
                  Background="#7F464646"
                  d:LayoutOverrides="VerticalAlignment">


                <Button x:Name="closeButton"
                        Width="25"
                        Height="25"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        BorderThickness="0"
                        Foreground="White"
                        Padding="0"
                        ToolTip="Close">
                    <materialDesign:PackIcon Kind="WindowClose" />
                </Button>
                <Button x:Name="minimizeButton"
                        Width="25"
                        Height="25"
                        Margin="0,0,25,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        BorderThickness="0"
                        Foreground="White"
                        Padding="0"
                        ToolTip="Minimize">
                    <materialDesign:PackIcon Kind="WindowMinimize" />
                </Button>
                <Label x:Name="devlabel"
                       Height="25"
                       Margin="5,0,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       VerticalContentAlignment="Center"
                       Content="{Binding versionSTRING}"
                       Foreground="#FFF6F3EA"
                       Padding="0" />
                <Button x:Name="improveButton"
                        Width="25"
                        Height="25"
                        Margin="0,0,75,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        Foreground="#DDFFFFFF"
                        Padding="0"
                        ToolTip="Innovation Hub">
                    <materialDesign:PackIcon x:Name="improveIcon" Kind="LightbulbOutline" />
                </Button>
                <Button x:Name="settingsButton"
                        Width="25"
                        Height="25"
                        Margin="0,0,50,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        Foreground="#DDFFFFFF"
                        Padding="0"
                        RenderTransformOrigin="0.5,0.5"
                        ToolTip="Settings">
                    <Button.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform />
                            <SkewTransform />
                            <RotateTransform />
                            <TranslateTransform />
                        </TransformGroup>
                    </Button.RenderTransform>
                    <materialDesign:PackIcon x:Name="settingsIcon" Kind="Settings" />
                </Button>
                <Button x:Name="submitReviewButton"
                        Width="25"
                        Height="25"
                        Margin="0,0,100,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        Foreground="#DDFFFFFF"
                        Padding="0"
                        ToolTip="Submit &amp; Review">
                    <materialDesign:PackIcon x:Name="submitReviewIcon" Kind="Timetable" />
                </Button>

