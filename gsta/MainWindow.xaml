                             HorizontalAlignment="Right"
                             VerticalAlignment="Bottom"
                             Text="{Binding password,
                                            Source={x:Static local:Settings.Default}}"
                             d:LayoutOverrides="VerticalAlignment" />
                    <TextBox x:Name="name1"
